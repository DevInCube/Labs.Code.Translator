<Window x:Class="My.Labs.Translator.Views.CodeGeneratorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CodeGeneratorWindow" Height="300" Width="300">
    <Grid Background="White">
        <TextBox Text="{Binding GenCode}" IsReadOnly="True" AcceptsReturn="True" Margin="5,5,5,10">
        </TextBox>
    </Grid>
</Window>
